    "@next/mdx": "^14.2.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/typography": "^0.5.13",
    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cobe": "^0.6.3",
    "date-fns": "^3.6.0",
    "dotted-map": "^2.2.3",
    "fast-glob": "^3.3.2",
    "framer-motion": "^11.18.2",
