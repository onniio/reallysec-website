{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "if exist .next rmdir /s /q .next",
    "clean:cache": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "clean:all": "if exist .next rmdir /s /q .next && if exist node_modules rmdir /s /q node_modules",
    "fresh:dev": "npm run clean && npm run dev",
    "fresh:build": "npm run clean && npm run build",
    "reinstall": "npm run clean:all && npm install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.2",
    "@mapbox/rehype-prism": "^0.9.0",
    "@mdx-js/loader": "^3.0.1",
